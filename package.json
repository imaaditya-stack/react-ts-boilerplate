{
  "name": "react-webpack-typescript-starterpack",
  "version": "1.0.0",
  "description": "React Webpack Typescript Starter Pack",
  "license": "MIT",
  "scripts": {
    "build:clean": "rimraf dist",
    "start": "webpack serve --config tools/webpack/webpack.config.dev.js",
    "build:dev": "npm run build:clean && webpack --config tools/webpack/webpack.config.dev.js --progress",
    "build:prod": "npm run build:clean && webpack --config tools/webpack/webpack.config.prod.js --progress",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/rimraf": "3.0.0",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.3.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^6.0.0",
    "logdown": "^3.3.1",
    "mini-css-extract-plugin": "^2.6.1",
    "react-refresh": "^0.14.0",
    "rimraf": "3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "4.7.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "core-js": "^3.27.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
